<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="filePathField" prefHeight="100.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.importController">
   <children>
      <TextField layoutX="11.0" layoutY="40.0" prefHeight="30.0" prefWidth="428.0" promptText="Path of file">
         <font>
            <Font name="Nirmala UI" size="14.0" />
         </font>
      </TextField>
      <Button layoutX="449.0" layoutY="40.0" mnemonicParsing="false" onAction="#importFile" text="Import">
         <font>
            <Font name="Nirmala UI" size="14.0" />
         </font>
      </Button>
      <Button layoutX="520.0" layoutY="40.0" mnemonicParsing="false" onAction="#goBack" text="Go Back">
         <font>
            <Font name="Nirmala UI" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
