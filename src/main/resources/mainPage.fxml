<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="420.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.mainPageController">
   <children>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#importFile" text="Import">
         <font>
            <Font name="Nirmala UI" size="13.0" />
         </font>
      </Button>
      <Button layoutX="81.0" layoutY="14.0" mnemonicParsing="false" onAction="#exportFile" text="Export">
         <font>
            <Font name="Nirmala UI" size="13.0" />
         </font>
      </Button>
      <TableView fx:id="itemTable" layoutY="58.0" prefHeight="298.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="itemName" prefWidth="159.20001220703125" text="Name" />
          <TableColumn fx:id="itemSerialNum" prefWidth="230.4000244140625" text="Serial Number" />
            <TableColumn fx:id="itemValue" prefWidth="211.99993896484375" text="Value" />
        </columns>
      </TableView>
      <TextField fx:id="searchField" layoutX="300.0" layoutY="14.0" prefHeight="27.0" prefWidth="286.0" promptText="Search for item...">
         <font>
            <Font name="Nirmala UI" size="13.0" />
         </font>
      </TextField>
      <Button layoutX="14.0" layoutY="378.0" mnemonicParsing="false" onAction="#newItem" text="New Item">
         <font>
            <Font name="Nirmala UI" size="13.0" />
         </font>
      </Button>
      <Button layoutX="97.0" layoutY="378.0" mnemonicParsing="false" onAction="#deleteItem" text="Delete Item">
         <font>
            <Font name="Nirmala UI" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
