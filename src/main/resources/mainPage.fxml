<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="420.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.mainPageController">
   <children>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#importFile" text="Import">
         <font>
            <Font name="Nirmala UI" size="13.0" />
         </font>
      </Button>
      <Button layoutX="81.0" layoutY="14.0" mnemonicParsing="false" onAction="#exportFile" text="Export">
         <font>
            <Font name="Nirmala UI" size="13.0" />
         </font>
      </Button>
      <TableView fx:id="itemTable" layoutY="62.0" prefHeight="297.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="itemName" prefWidth="159.20001220703125" text="Name" />
          <TableColumn fx:id="itemSerialNum" prefWidth="230.4000244140625" text="Serial Number" />
            <TableColumn fx:id="itemValue" prefWidth="211.99993896484375" text="Value" />
        </columns>
      </TableView>
      <Button layoutX="530.0" layoutY="14.0" mnemonicParsing="false" onAction="#itemSearch" text="Search">
         <font>
            <Font name="Nirmala UI" size="13.0" />
         </font>
      </Button>
      <TextField fx:id="searchField" layoutX="299.0" layoutY="14.0" prefHeight="27.0" prefWidth="222.0" promptText="Item's name, serial number or value">
         <font>
            <Font name="Nirmala UI" size="13.0" />
         </font>
      </TextField>
      <Label layoutX="14.0" layoutY="382.0" text="Sort By:">
         <font>
            <Font name="Nirmala UI" size="13.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="sortChoiceBox" layoutX="67.0" layoutY="379.0" prefWidth="150.0" />
   </children>
</AnchorPane>
